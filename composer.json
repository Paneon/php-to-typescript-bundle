{
    "name": "paneon/php-to-typescript-bundle",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "description": "Symfony-Bundle to use the PHP to Typescript library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Paneon\\PhpToTypeScriptBundle\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Alexander Pape",
            "email": "300609+Macavity@users.noreply.github.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "paneon/php-to-typescript": "^2.2.0",
        "symfony/console": "*",
        "symfony/framework-bundle": "*",
        "symfony/monolog-bundle": "*",
        "nikic/php-parser": "^5.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.2",
        "phpunit/phpunit": "^10.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Paneon\\PhpToTypeScriptBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "build": [
            "@lint"
        ],
        "lint": [
            "@php vendor/bin/phpstan analyze src --level=5"
        ],
        "test": [
            "@php vendor/bin/phpunit"
        ]
    }
}

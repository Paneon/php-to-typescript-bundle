{
    "name": "paneon/php-to-typescript-bundle",
    "minimum-stability": "stable",
    "description": "Symfony-Bundle to use the PHP to Typescript library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Paneon\\PhpToTypeScriptBundle\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Alexander Pape",
            "email": "300609+Macavity@users.noreply.github.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "paneon/php-to-typescript": "^2.0",
        "symfony/console": "*",
        "symfony/framework-bundle": "*",
        "symfony/monolog-bundle": "*"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Paneon/php-to-typescript.git"
        }
    ],
    "require-dev": {
        "phpstan/phpstan": "^1.2"
    },
    "scripts": {
        "build": [
            "@lint"
        ],
        "lint": [
            "@php vendor/bin/phpstan analyze src --level=5"
        ]
    }
}
